        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.VisualStudio.SharePoint.Packages.ISafeControl">
          <source>Represents a safe control in the package.</source>
          <target state="translated">表示包中的安全控件。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.VisualStudio.SharePoint.Packages.ISafeControl">
          <source>Safe controls are controls that marked as secure by the control's author.</source>
          <target state="translated">安全控件是控件，标记为安全的控件的作者。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.VisualStudio.SharePoint.Packages.ISafeControl">
          <source>Safe controls can be used by contributors.</source>
          <target state="translated">安全控件可由 contributors （参与者）。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.VisualStudio.SharePoint.Packages.ISafeControl">
