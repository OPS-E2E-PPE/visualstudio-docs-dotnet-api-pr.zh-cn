{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**",
          "add/metadata/**",
          "api/overwrites/**"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": ".",
        "dest": "vssdk_net_toc"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": {
        "files": [
          "api/overwrites/*.md"
        ]
      },
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "ROBOTS": "INDEX,FOLLOW",
      "author": "gregvanl",
      "ms.author": "gregvanl",
      "manager": "ghogen",
      "ms.date": "11/16/2016",
      "ms.topic": "article",
      "audience": "developer",
      "ms.reviewer": "",
      "ms.prod": "visual-studio-dev15",
      "ms.technology": "vs-ide-sdk",
      "ms.tgt_pltfrm": "dotnet",
      "searchScope": ["VS SDK"],
      "apiPlatform": "dotnet"
    },
    "fileMetadata": {},
    "template": [],
    "xref": ["msdn.4.5.2.zip", "namespaces.4.5.2.zip", "vs.110.zip", "vs.140.zip", "office15.zip"],
    "dest": "dotnet-vssdk",
    "lruSize": 0
  }
}
